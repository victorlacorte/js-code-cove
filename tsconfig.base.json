{
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "allowImportingTsExtensions": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "module": "nodenext",
    "lib": ["esnext"],
    "target": "es2022",
    "types": ["node"],
    "skipLibCheck": true
  }
}
